#ifdef _WIN32
    SetConsoleOutputCP(CP_UTF8);
#endif

    std::ofstream output_file{&quot;image.ppm&quot;, std::ios::out | std::ios::binary};
    if (!output_file) {
        std::cerr &lt;&lt; &quot;Error: Could not open file for writing.\n&quot;;
        return EXIT_FAILURE;
    }

    // Image

    int image_width = 256;
    int image_height = 256;

    // Render

    output_file &lt;&lt; &quot;P3\n&quot; &lt;&lt; image_width &lt;&lt; ' ' &lt;&lt; image_height &lt;&lt; &quot;\n255\n&quot;;

    for (int j = 0; j &lt; image_height; j++) {
        for (int i = 0; i &lt; image_width; i++) {
            auto r = double(i) / (image_width-1);
            auto g = double(j) / (image_height-1);
            auto b = 0.0;

            int ir = int(255.999 * r);
            int ig = int(255.999 * g);
            int ib = int(255.999 * b);

            output_file &lt;&lt; ir &lt;&lt; ' ' &lt;&lt; ig &lt;&lt; ' ' &lt;&lt; ib &lt;&lt; '\n';
        }
    }
    
    output_file.flush();
    output_file.close();

    return EXIT_SUCCESS;
}"><pre class="notranslate">#<span class="pl-k">include</span> <span class="pl-s"><span class="pl-pds">&lt;</span>iostream<span class="pl-pds">&gt;</span></span>
